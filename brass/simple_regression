from sklearn.linear_model import LinearRegression
import numpy as np

# Prepare the data for regression
X = aligned_import_counts.values.reshape(-1, 1)  # Reshape for sklearn
y = aligned_brass_ppi['Brass PPI'].values

# Fit the linear regression model
model = LinearRegression()
model.fit(X, y)

# Predict the brass PPI for an import count of 125
predicted_ppi = model.predict(np.array([[125]]))

predicted_ppi
